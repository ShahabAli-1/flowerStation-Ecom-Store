{{ 'faqcomplaints-bridalbouquets.css' | asset_url | stylesheet_tag }}
<div class="faq-bridalbouquets">
  <h2>{{ section.settings.faqc-bb-title }}</h2>
  <slider-component class="slider-mobile-gutter slider-component-full-width slider-component-desktop scroll-trigger animate--slide-in">
    <div class="faq-cardcontainer">
      <ul
        id="Slider-{{ section.id }}"
        class="grid product-grid contains-card contains-card--product contains-card--standard grid--1-col-desktop grid--1-col-tablet-down slider slider--desktop slider--tablet grid--peek"
        role="list"
        aria-label="{{ 'general.slider.name' | t }}">
        <li
          id="Slide-{{ section.id }}-1"
          class="grid__item scroll-trigger animate--slide-in"
          data-cascadestyle="--animation-order: 1;">
          <div class="card">
            {% if section.settings.faqc-bb-image1 != blank %}
              <img src="{{ section.settings.faqc-bb-image1 | img_url: 'master' }}" alt="flower picture">
            {% else %}
              <img src="{{'faq-bridal-bouquets2.png' | asset_url}}" alt="flower picture">
            {% endif %}
            <p>{{ section.settings.faqc-bb-image1-title }}</p>
            <h6>{{ section.settings.faqc-bb-image1-price }}</h6>
          </div>
        </li>
        <li
          id="Slide-{{ section.id }}-2"
          class="grid__item scroll-trigger animate--slide-in"
          data-cascadestyle="--animation-order: 2;">
          <div class="card">
            {% if section.settings.faqc-bb-image2 != blank %}
              <img src="{{ section.settings.faqc-bb-image2 | img_url: 'master' }}" alt="flower picture">
            {% else %}
              <img src="{{'faq-bridal-bouquets1.png' | asset_url}}" alt="flower picture">
            {% endif %}
            <p>{{ section.settings.faqc-bb-image2-title }}</p>
            <h6>{{ section.settings.faqc-bb-image2-price }}</h6>
          </div>
        </li>
        <li
          id="Slide-{{ section.id }}-2"
          class="grid__item scroll-trigger animate--slide-in"
          data-cascadestyle="--animation-order: 2;">
          <div class="card">
            {% if section.settings.faqc-bb-image3 != blank %}
              <img src="{{ section.settings.faqc-bb-image3 | img_url: 'master' }}" alt="flower picture">
            {% else %}
              <img src="{{'faq-bridal-bouquets3.png' | asset_url}}" alt="flower picture">
            {% endif %}
            <p>{{ section.settings.faqc-bb-image3-title }}</p>
            <h6>{{ section.settings.faqc-bb-image3-price }}</h6>
          </div>
        </li>
      </ul>
      <div class="bridal-buttoncon">
        <button
          type="button"
          class="slider-button slider-button--prev"
          name="previous"
          aria-label="{{ 'general.slider.previous_slide' | t }}"
          aria-controls="Slider-{{ section.id }}">
          {% render 'icon-caret' %}
        </button>
        <button
          type="button"
          class="slider-button slider-button--next"
          name="next"
          aria-label="{{ 'general.slider.next_slide' | t }}"
          aria-controls="Slider-{{ section.id }}">
          {% render 'icon-caret' %}
        </button>
      </div>

    </div>
  </slider-component>
</div>


{% schema %}
  {
    "name": "faqc-bridalbouquets",
    "settings": [
      {
        "type": "text",
        "id": "faqc-bb-title",
        "label": "main title",
        "default": "Bridal Bouquets"
      },
      {
        "type": "image_picker",
        "id": "faqc-bb-image1",
        "label": "image1"
      },
      {
        "type": "text",
        "id": "faqc-bb-image1-title",
        "label": "image1 title",
        "default": "Bridal Bouquets"
      },
      {
        "type": "text",
        "id": "faqc-bb-image1-price",
        "label": "image1 price",
        "default": "$100"
      }, {
        "type": "image_picker",
        "id": "faqc-bb-image2",
        "label": "image2"
      }, {
        "type": "text",
        "id": "faqc-bb-image2-title",
        "label": "image2 title",
        "default": "Bridal Bouquets"
      }, {
        "type": "text",
        "id": "faqc-bb-image2-price",
        "label": "image2 price",
        "default": "$100"
      }, {
        "type": "image_picker",
        "id": "faqc-bb-image3",
        "label": "image3"
      }, {
        "type": "text",
        "id": "faqc-bb-image3-title",
        "label": "image3 title",
        "default": "Bridal Bouquets"
      }, {
        "type": "text",
        "id": "faqc-bb-image3-price",
        "label": "image3 price",
        "default": "$100"
      }
    ]
  }
{% endschema %}