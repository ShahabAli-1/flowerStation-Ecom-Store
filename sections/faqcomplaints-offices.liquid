      <p>{{ section.settings.faqcomplaints-offices-image4-text1 }}</p>
      <h3>{{ section.settings.faqcomplaints-offices-image4-heading2 }}</h3>
      <p>{{ section.settings.faqcomplaints-offices-image4-text2 }}</p>
    </div>
  </div>
