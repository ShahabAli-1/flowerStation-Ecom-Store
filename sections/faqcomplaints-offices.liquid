{{ 'faqcomplaints-offices.css' | asset_url | stylesheet_tag }}
<div class="faqcomplaints-offices">
  <h2>{{ section.settings.faqcomplaints-offices-title }}</h2>
  <p>{{ section.settings.faqcomplaints-offices-subtitle }}</p>
  <div class="faqcomplaints-office">
    <div class="faqcomplaints-left">
      {% if section.settings.faqcomplaints-offices-image1 != blank %}
        <img src="{{ section.settings.faqcomplaints-offices-image1 | img_url: 'master' }}" alt="offices">
      {% else %}
        <img src="{{ 'faqcomplaints-office1.png' | asset_url}}" alt="offices">
      {% endif %}
    </div>
    <div class="faqcomplaints-right">
      <h3>{{ section.settings.faqcomplaints-offices-image1-heading1 }}</h3>
      <p>{{ section.settings.faqcomplaints-offices-image1-text1 }}</p>
      <h3>{{ section.settings.faqcomplaints-offices-image1-heading2 }}</h3>
      <p>{{ section.settings.faqcomplaints-offices-image1-text2 }}</p>
    </div>
  </div>
  <div class="faqcomplaints-office">
    <div class="faqcomplaints-left">
      {% if section.settings.faqcomplaints-offices-image2 != blank %}
        <img src="{{ section.settings.faqcomplaints-offices-image2 | img_url: 'master' }}" alt="offices">
      {% else %}
        <img src="{{ 'faqcomplaints-office2.png' | asset_url}}" alt="offices">
      {% endif %}
    </div>
    <div class="faqcomplaints-right">
      <h3>{{ section.settings.faqcomplaints-offices-image2-heading1 }}</h3>
      <p>{{ section.settings.faqcomplaints-offices-image2-text1 }}</p>
      <h3>{{ section.settings.faqcomplaints-offices-image2-heading2 }}</h3>
      <p>{{ section.settings.faqcomplaints-offices-image2-text2 }}</p>
    </div>
  </div>
  <div class="faqcomplaints-office">
    <div class="faqcomplaints-left">
      {% if section.settings.faqcomplaints-offices-image3 != blank %}
        <img src="{{ section.settings.faqcomplaints-offices-image3 | img_url: 'master' }}" alt="offices">
      {% else %}
        <img src="{{ 'faqcomplaints-office3.png' | asset_url}}" alt="offices">
      {% endif %}
    </div>
    <div class="faqcomplaints-right">
      <h3>{{ section.settings.faqcomplaints-offices-image3-heading1 }}</h3>
      <p>{{ section.settings.faqcomplaints-offices-image3-text1 }}</p>
      <h3>{{ section.settings.faqcomplaints-offices-image3-heading2 }}</h3>
      <p>{{ section.settings.faqcomplaints-offices-image3-text2 }}</p>
    </div>
  </div>
  <div class="faqcomplaints-office">
    <div class="faqcomplaints-left">
      {% if section.settings.faqcomplaints-offices-image4 != blank %}
