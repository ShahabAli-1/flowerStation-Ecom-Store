{{ 'delivery-northwest.css' | asset_url | stylesheet_tag }}
<div class="delivery-northwest">
  <h2>
    <span>
      {{ section.settings['delivery-northwest-title1'] }}
    </span>
    {{
       section.settings['delivery-northwest-title2'] }}</h2>
  <p>
    <span>
      {{ section.settings['delivery-northwest-subtitle1'] }}
    </span>
    {{ section.settings['delivery-northwest-subtitle2'] }}
  </p>
  <h3>{{ section.settings['delivery-northwest-best-selling-flowers'] }}</h3>
  <svg
    viewBox="0 0 317 30"
    fill="none"
    xmlns="http://www.w3.org/2000/svg"
    xmlns:xlink="http://www.w3.org/1999/xlink">
    <g style="mix-blend-mode:multiply">
      <rect
        width="317"
        height="30"
        fill="url(#pattern0)" />
    </g>
    <defs>
      <pattern
        id="pattern0"
        patternContentUnits="objectBoundingBox"
        width="1"
        height="1">
        <use xlink:href="#image0_4_203" transform="matrix(0.00204499 0 0 0.0216087 0 -0.00780504)" />
      </pattern>
      <image
        id="image0_4_203"
        width="489"
        height="47"
        xlink:href="data:image/png;base64," />
    </defs>
  </svg>
</div>
{% schema %}
  {
    "name": "Delivery North West",
    "settings": [
      {
        "type": "text",
        "id": "delivery-northwest-title1",
        "label": "Title 1",
        "default": "Flower Delivery"
      },
      {
        "type": "text",
        "id": "delivery-northwest-title2",
        "label": "Title 2",
        "default": "In North West London"
      },
      {
        "type": "text",
        "id": "delivery-northwest-subtitle1",
        "label": "Subtitle 1",
        "default": "Flower Delivery in North West London Fresh Flowers & Gifts"

      },
      {
        "type": "text",
        "id": "delivery-northwest-subtitle2",
        "label": "Subtitle 2",
        "default": "Delivered Fast and Easy"
      }, {
        "type": "text",
        "id": "delivery-northwest-best-selling-flowers",
        "label": "Best Selling Flowers",
        "default": "Best Selling Flowers"
      }

    ]
  }
{% endschema %}