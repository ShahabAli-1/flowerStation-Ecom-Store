{{ 'delivery-internationl-delivery.css' | asset_url | stylesheet_tag }}
<div class="delivery-international">
  <h2>{{ section.settings['international-delivery-title'] }}</h2>
  <p>{{ section.settings['international-delivery-p1'] }}</p>
  <p>{{ section.settings['international-delivery-p2'] }}</p>
  <p>{{ section.settings['international-delivery-p3'] }}</p>
  <p>{{ section.settings['international-delivery-p4'] }}</p>
  <p>{{ section.settings['international-delivery-p5'] }}</p>
  <div class="delivery-button-continer1">
    <button>
      <svg
        width="16"
        height="18"
        viewBox="0 0 16 18"
        fill="none"
        xmlns="http://www.w3.org/2000/svg">
        <path
          d="M14.9996 13.0943V15.5261C15.0004 15.7518 14.9602 15.9753 14.8817 16.1821C14.8031 16.389 14.6879 16.5746 14.5435 16.7273C14.399 16.8799 14.2285 16.9961 14.0428 17.0684C13.8571 17.1407 13.6603 17.1676 13.465 17.1473C11.299 16.8762 9.2183 16.0239 7.39023 14.6587C5.68945 13.4142 4.24748 11.7537 3.16674 9.79511C1.9771 7.68041 1.23677 5.27276 1.00571 2.76719C0.988123 2.54303 1.01126 2.31711 1.07364 2.10381C1.13603 1.89052 1.2363 1.69451 1.36807 1.52828C1.49984 1.36206 1.66022 1.22924 1.839 1.1383C2.01779 1.04736 2.21106 1.00029 2.40651 1.00008H4.51826C4.85987 0.996207 5.19105 1.13551 5.45007 1.39203C5.70909 1.64855 5.87828 2.00478 5.92609 2.39431C6.01522 3.17255 6.18052 3.93667 6.41883 4.6721C6.51354 4.96224 6.53403 5.27755 6.47789 5.58069C6.42175 5.88383 6.29132 6.16208 6.10207 6.38247L5.20809 7.41194C6.21016 9.44132 7.6693 11.1216 9.43159 12.2755L10.3256 11.2461C10.517 11.0281 10.7586 10.8779 11.0218 10.8133C11.2851 10.7486 11.5589 10.7723 11.8108 10.8813C12.4495 11.1557 13.113 11.3461 13.7888 11.4487C14.1308 11.5043 14.4431 11.7026 14.6663 12.006C14.8895 12.3094 15.0081 12.6967 14.9996 13.0943Z"
          stroke="#2D2D2D"
          stroke-linecap="round"
          stroke-linejoin="round" />
      </svg>
      Call Now
    </button>
    <button>
      <svg
        width="15"
        height="12"
        viewBox="0 0 15 12"
        fill="none"
        xmlns="http://www.w3.org/2000/svg">
        <path
          d="M1.9 0.678711H13.1C13.87 0.678711 14.5 1.27735 14.5 2.00903V9.99093C14.5 10.7226 13.87 11.3212 13.1 11.3212H1.9C1.13 11.3212 0.5 10.7226 0.5 9.99093V2.00903C0.5 1.27735 1.13 0.678711 1.9 0.678711Z"
          stroke="white"
          stroke-linecap="round"
          stroke-linejoin="round" />
        <path
          d="M14.5 2.00879L7.5 6.6649L0.5 2.00879"
          stroke="white"
          stroke-linecap="round"
          stroke-linejoin="round" />
      </svg>

      Email Now
    </button>
  </div>
  <h6>
    <a href="{{ section.settings['international-delivery-url'] }}">{{ section.settings['international-delivery-button'] }}</a>
  </h6>
</div>
{% schema %}
  {
    "name": "international-delivery",
    "settings": [
      {
        "type": "text",
        "id": "international-delivery-title",
        "label": "Title",
        "default": "International Delivery"
      },
      {
        "type": "text",
        "id": "international-delivery-p1",
        "label": "Paragraph 1",
        "default": "We can help you with any international delivery."

      },
      {
        "type": "text",
        "id": "international-delivery-p2",
        "label": "Paragraph 2",
        "default": "Just get in touch with our customer service team and we will find the best option for you."
      },
      {
        "type": "text",
        "id": "international-delivery-p3",
        "label": "Paragraph 3"
      }, {
        "type": "text",
        "id": "international-delivery-p5",
        "label": "Paragraph 5"
      }, {
        "type": "text",
        "id": "international-delivery-p4",
        "label": "Paragraph 4"
      }, {
        "type": "text",
        "id": "international-delivery-button",
        "label": "Button",
        "default": "Check availability for the Christmas period here"
      }, {
        "type": "text",
        "id": "international-delivery-url",
        "label": "URL",
        "default": "#delivery_holidays"
      }
    ]
  }
{% endschema %}